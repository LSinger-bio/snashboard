library(httr)
library(jsonlite)
# Fetch GBIF data
url <- "https://api.gbif.org/v1/occurrence/search?mediaType=StillImage&scientificName=Gastropoda"
res <- httr::GET(url)
gbif_data <- jsonlite::fromJSON(content(res, "text"))
# Explore data structure
str(gbif_data$results)
View(as.data.frame(gbif_data$results))
